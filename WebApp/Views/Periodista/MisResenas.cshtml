@model IEnumerable<Dominio.Resena>

<h1>Mis reseñas</h1>

<table class="table">
    <tr class="table-info">
        <th>Título</th>
        <th>Contenido</th>
        <th>Selección 1</th>
        <th>Selección 2</th>
    </tr>

    @foreach (var r in Model)
    {
        <tr>
            <td>@r.Titulo</td>
            <td>@r.Contenido</td>
            <td>@r.Partido.Seleccion1.Pais.Nombre</td>
            <td>@r.Partido.Seleccion2.Pais.Nombre</td>
        </tr>
    }
</table>

<div>
    <a asp-controller="Home" asp-action="Index">Atrás</a>
</div>